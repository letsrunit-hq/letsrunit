{
  "name": "@letsrunit/controller",
  "version": "0.2.5",
  "description": "Browser automation controller for letsrunit",
  "keywords": [
    "testing",
    "automation",
    "controller",
    "playwright",
    "letsrunit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letsrunit-hq/letsrunit.git",
    "directory": "packages/controller"
  },
  "bugs": "https://github.com/letsrunit-hq/letsrunit/issues",
  "homepage": "https://github.com/letsrunit-hq/letsrunit#readme",
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/tsup",
    "test": "vitest run --environment jsdom",
    "test:cov": "vitest run --environment jsdom --coverage",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "@letsrunit/bdd": "workspace:*",
    "@letsrunit/gherker": "workspace:*",
    "@letsrunit/gherkin": "workspace:*",
    "@letsrunit/journal": "workspace:*",
    "@letsrunit/playwright": "workspace:*",
    "@letsrunit/utils": "workspace:*",
    "@playwright/test": "^1.57.0",
    "jsdom": "^27.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  }
}
