{
  "name": "@letsrunit/executor",
  "version": "0.2.4",
  "description": "High-level test execution orchestrator for letsrunit",
  "keywords": [
    "testing",
    "executor",
    "automation",
    "letsrunit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letsrunit-hq/letsrunit.git",
    "directory": "packages/executor"
  },
  "bugs": "https://github.com/letsrunit-hq/letsrunit/issues",
  "homepage": "https://github.com/letsrunit-hq/letsrunit#readme",
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/tsup",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "@letsrunit/ai": "workspace:*",
    "@letsrunit/controller": "workspace:*",
    "@letsrunit/gherkin": "workspace:*",
    "@letsrunit/journal": "workspace:*",
    "@letsrunit/mailbox": "workspace:*",
    "@letsrunit/playwright": "workspace:*",
    "@letsrunit/utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  }
}
