{
  "name": "@letsrunit/playwright",
  "version": "0.2.6",
  "description": "Playwright extensions and utilities for letsrunit",
  "keywords": [
    "testing",
    "playwright",
    "automation",
    "letsrunit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letsrunit-hq/letsrunit.git",
    "directory": "packages/playwright"
  },
  "bugs": "https://github.com/letsrunit-hq/letsrunit/issues",
  "homepage": "https://github.com/letsrunit-hq/letsrunit#readme",
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/tsup",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "@letsrunit/utils": "workspace:*",
    "@playwright/test": "^1.57.0",
    "case": "^1.6.3",
    "diff": "^8.0.3",
    "fast-json-stable-stringify": "^2.1.0",
    "jsdom": "^27.4.0",
    "metascraper-description": "^5.49.15",
    "metascraper-image": "^5.49.15",
    "metascraper-logo": "^5.49.15",
    "metascraper-logo-favicon": "^5.49.15",
    "metascraper-title": "^5.49.15",
    "metascraper-url": "^5.49.15",
    "rehype-format": "^5.0.1",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "vitest": "^4.0.17"
  },
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  }
}
