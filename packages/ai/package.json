{
  "name": "@letsrunit/ai",
  "version": "0.2.4",
  "description": "AI integration for test generation with letsrunit",
  "keywords": [
    "testing",
    "ai",
    "llm",
    "letsrunit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letsrunit-hq/letsrunit.git",
    "directory": "packages/ai"
  },
  "bugs": "https://github.com/letsrunit-hq/letsrunit/issues",
  "homepage": "https://github.com/letsrunit-hq/letsrunit#readme",
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/tsup",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "@ai-sdk/openai": "^2.0.89",
    "@ai-sdk/togetherai": "^1.0.31",
    "@letsrunit/utils": "workspace:*",
    "ai": "^5.0.121",
    "cache-manager": "^7.2.8",
    "iso-639-1": "^3.1.5",
    "langsmith": "^0.4.7",
    "mustache": "^4.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.6",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "4.0.17",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  }
}
