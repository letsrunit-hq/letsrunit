{
  "name": "@letsrunit/gherkin",
  "version": "0.2.0",
  "description": "Gherkin types and utilities for letsrunit",
  "keywords": [
    "testing",
    "gherkin",
    "bdd",
    "letsrunit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letsrunit/letsrunit.git",
    "directory": "packages/gherkin"
  },
  "bugs": "https://github.com/letsrunit/letsrunit/issues",
  "homepage": "https://github.com/letsrunit/letsrunit#readme",
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/tsup",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "build:locator-parser": "peggy --format es src/locator/parser.peggy"
  },
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "@cucumber/cucumber-expressions": "^18.1.0",
    "@cucumber/gherkin": "^37.0.1",
    "@cucumber/messages": "^31.2.0",
    "@letsrunit/utils": "workspace:*"
  },
  "devDependencies": {
    "peggy": "^5.0.6",
    "vitest": "^4.0.17"
  },
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  }
}
