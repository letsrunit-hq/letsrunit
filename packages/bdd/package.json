{
  "name": "@letsrunit/bdd",
  "version": "0.2.4",
  "description": "BDD step definitions for browser automation with letsrunit",
  "keywords": [
    "testing",
    "bdd",
    "gherkin",
    "playwright",
    "letsrunit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letsrunit-hq/letsrunit.git",
    "directory": "packages/bdd"
  },
  "bugs": "https://github.com/letsrunit-hq/letsrunit/issues",
  "homepage": "https://github.com/letsrunit-hq/letsrunit#readme",
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/tsup",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "gen:stub": "tsx scripts/generate-stub.ts"
  },
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "@cucumber/cucumber-expressions": "^18.1.0",
    "@letsrunit/gherkin": "workspace:*",
    "@letsrunit/mailbox": "workspace:*",
    "@letsrunit/playwright": "workspace:*",
    "@letsrunit/utils": "workspace:*",
    "@playwright/test": "^1.57.0",
    "iso-639-1": "^3.1.5",
    "metascraper": "^5.49.15",
    "metascraper-lang": "^5.49.15"
  },
  "peerDependencies": {
    "@cucumber/cucumber": "^12.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.5.0",
    "vitest": "^4.0.17"
  },
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  }
}
