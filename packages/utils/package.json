{
  "name": "@letsrunit/utils",
  "version": "0.2.1",
  "description": "Utility functions for letsrunit",
  "keywords": [
    "testing",
    "utilities",
    "letsrunit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letsrunit/letsrunit.git",
    "directory": "packages/utils"
  },
  "bugs": "https://github.com/letsrunit/letsrunit/issues",
  "homepage": "https://github.com/letsrunit/letsrunit#readme",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/tsup",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "fast-json-stable-stringify": "^2.1.0",
    "lru-cache": "^11.2.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "vitest": "^4.0.17"
  }
}
