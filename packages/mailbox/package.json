{
  "name": "@letsrunit/mailbox",
  "version": "0.2.2",
  "description": "Test email mailbox integration for letsrunit",
  "keywords": [
    "testing",
    "email",
    "mailbox",
    "letsrunit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/letsrunit/letsrunit.git",
    "directory": "packages/mailbox"
  },
  "bugs": "https://github.com/letsrunit/letsrunit/issues",
  "homepage": "https://github.com/letsrunit/letsrunit#readme",
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/tsup",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc--noEmit"
  },
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "@letsrunit/utils": "workspace:*",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0"
  },
  "devDependencies": {
    "vitest": "^4.0.17"
  },
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  }
}
